{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Price": {
      "type": "biginteger",
      "min": "1"
    },
    "SubName": {
      "type": "string"
    },
    "Desc": {
      "type": "richtext"
    },
    "Photo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
